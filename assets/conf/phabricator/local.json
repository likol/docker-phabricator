{
  "environment.append-paths": [
    "\/usr\/lib\/git-core\/",
    "\/bin",
    "\/usr\/bin",
    "\/usr\/local\/bin"
  ],
  "phpmailer.smtp-password": "{{SMTP_PASS}}",
  "phpmailer.smtp-user": "{{SMTP_USER}}",
  "phpmailer.smtp-protocol": "{{SMTP_PROTOCOL}}",
  "phpmailer.smtp-port": {{SMTP_PORT}},
  "phpmailer.smtp-host": "{{SMTP_HOST}}",
  "metamta.mail-adapter": "{{MAIL_ADAPTER}}",
  "security.alternate-file-domain": "http:\/\/{{PHD_HOST}}",
  "storage.local-disk.path": "\/var\/phd\/storage",
  "phabricator.base-uri": "http:\/\/{{PHD_HOST}}\/",
  "phabricator.timezone": "{{PHD_TIMEZONE}}",
  "mysql.pass": "{{DB_PASS}}",
  "mysql.user": "{{DB_USER}}",
  "mysql.host": "{{DB_HOST}}",
  "phd.log-directory": "{{PHD_LOG_DIR}}",
  "repository.default-local-path": "/var/phd/repo",
  "pygments.enabled": true,
  "diffusion.allow-http-auth": {{ALLOW_HTTP_AUTH}},
  "diffusion.ssh-port": {{SSH_PORT}},
  "diffusion.ssh-user": "git",
  "phabricator.timezone": "{{PHD_TIMEZONE}}",
  "log.access.path": "/var/log/phd/phd-web-access.log",
  "log.ssh.path": "/var/log/phd/phd-ssh-access.log",
  "phd.user": "git"
}